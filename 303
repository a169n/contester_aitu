#include <iostream>
#include <cmath>
using namespace std;

int main() {
  double a, b, c, D;
  cin >> a >> b >> c;
  D = b * b - (4 * a * c);
  if (D < 0) {
    cout << "no solution";
  }
    else if (D == 0) {
      cout << (-b + 0) / (2 * a);
    }
      else {
        double x1 = ( - b + sqrt(D)) / (2 * a);
        double x2 = ( - b - sqrt(D)) / (2 * a);
        cout << x1 << " " << x2;
        }
}
