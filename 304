#include <iostream>
using namespace std;
int main() {
  double a, b, c;
  cin >> a >> b >> c;
    double min = a, max = a;
    if (min > b) {
        min = b;
    }
    if (min > c) {
        min = c;
    }
    if (max < b) {
        max = b;
    }
    if (max < c) {
        max = c;
    }
    cout << min << " " << max;
    return 0;
}
